	-unity-text-align:middle-center;
	color:rgb(255,255,255);
}

.UdonNode:hover #help-button > #icon {
	visibility:visible;
}

.UdonNode #help-button:hover > #icon {
	opacity:1;
}

.UdonNode #contents > #top > #input {
	flex: 1 1 auto;
}

.UdonNode PopupField {
